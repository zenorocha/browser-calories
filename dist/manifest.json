{
  "manifest_version": 2,
  "name": "Browser Calories",
  "description": "Measure your performance budget",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16" : "images/icon-16.png",
    "48" : "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
